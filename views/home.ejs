			$http.get("view_notif").success(function(msg){
				$scope.video = msg;
				socket.emit('view_notif',msg)
			});
		}
		$scope.getdata=function(){
			$http.get("get_notif").success(function(msg){
				socket.emit("send_notif_pesan",msg);
			});
		}
		$scope.getdata();
		$scope.submit=function(){
		var status = "lama";
		$http.post("ubah_notif",{status:status}).success(function(){
		$scope.getdata1();
		});
		}
		$scope.getdata1();
socket.on('add_notif', function(msg) {
    $scope.$apply(function () {
      $scope.notif1 = msg.length;
 })
  });
  $scope.lihat_notif_pesan=function(){
  $http.post("ubah_notif_pesan",{status:"lama"}).success(function(){
  })
  $http.get("get_pesan_li").success(function(msg){
  $scope.pesan = msg;
  $scope.getdata();
  })
  }
  socket.on('terimapesan', function(msg) {
    $scope.$apply(function () {
      $scope.notif = msg.length;
 })
  });
});

